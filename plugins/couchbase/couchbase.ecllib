// Record structure that will be used to return Kafka messages to ECL
EXPORT CouchbaseMessage := RECORD
    STRING      jsonmessage;
END;

EXPORT couchbaseembed := SERVICE : plugin('couchbaseembed'), namespace('couchbaseembed')
  STREAMED DATASET(CouchbaseMessage) executen1ql(CONST VARSTRING host, CONST VARSTRING bucket, CONST VARSTRING query) : cpp,action,context,entrypoint='executen1ql';
END;
